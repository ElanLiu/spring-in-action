<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>

	<bean id="greeting" class="com.springinaction.di.impl.GreetingImpl">
		<property name="greeting" value="Elan" />
	</bean>
	
	<bean id="greetingConstructor" class="com.springinaction.di.impl.GreetingImpl">
		<constructor-arg value="Alex" />
	</bean>
	

</beans>